{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "cf_ai_habit_coach",
  "main": "src/worker.ts",
  "compatibility_date": "2025-11-01",

  // Serve static UI from ./public
  "assets": {
    "directory": "public"
  },

  "ai": {
    "binding": "AI"
  },

  "durable_objects": {
    "bindings": [
      {
        "name": "HABIT_STORE",
        "class_name": "HabitStore"
      }
    ]
  },

  "migrations": [
    {
      "tag": "v1",
      "new_classes": ["HabitStore"]
    }
  ]
}
